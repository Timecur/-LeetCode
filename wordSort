#include <iostream>
#include <string>
#include <vector>
#include <ctype.h>

using namespace std;

int main()
{
	string str;
	while (getline(cin, str)){
		string ret = "";
		vector<string> v;
		for (int i = 0; i < str.size(); i++){
			if (isalpha(str[i]) > 0){
				ret.push_back(str[i]);
			}
			else{
				v.push_back(ret);
				ret.clear();
			}
		}
		v.push_back(ret);
		ret.clear();

		for (int i = (v.size() - 1); i >= 0; i--){
			if (ret.empty()){
				ret = v[i];
			}
			else{
				ret += " " + v[i];
			}
		}
		
		cout << ret << endl;
	}

	return 0;
}
